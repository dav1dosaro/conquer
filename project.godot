; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Capture"
run/main_scene="res://src/scenes/Game.tscn"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.0627451, 0.0627451, 0.0627451, 1 )
config/icon="res://src/images/icon.png"

[autoload]

Global="*res://src/scripts/Global.gd"
Scenes="*res://src/scripts/Scenes.gd"
Scripts="*res://src/scripts/Scripts.gd"
Signals="*res://src/scripts/Signals.gd"
Methods="*res://src/scripts/Methods.gd"

[display]

window/size/width=1280
window/size/height=1024

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://src/default_env.tres"
